<?xml version="1.0" encoding="ISO-8859-1"?>
<?xml-stylesheet type="text/xsl" href="http://www.jayasoft.fr/org/ivyrep/ivy-doc.xsl"?>
<ivy-module version="1.3">
    <info 
        organisation="Apache"
        module="ivy"
        revision="2.2.0"
        status="release">
        <license name="Apache 2.0" url="http://ant.apache.org/ivy/license.html"/>
        <description homepage="http://ant.apache.org/ivy/">
            Dependency manager 
        </description>
        </info>
    <configurations>
        <conf name="default"/>
        <conf name="templates"/>
        <conf name="runtime-templates"/>
    </configurations>
    <publications>
        <artifact name="ivy" type="jar" conf="default"/>
        <artifact name="ivy-report" type="css" conf="templates,runtime-templates"/>
        <artifact name="ivy-report" type="xsl" conf="templates"/>
        <artifact name="rt-report" type="xsl" conf="runtime-templates"/>
    </publications>
</ivy-module>
