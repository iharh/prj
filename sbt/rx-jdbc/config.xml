        <resource kind="regexp">reg_social_media</resource>
        <resource kind="regexp" type="Emoticon">reg_social_media_smiley</resource>
	<resource kind="regexp" type="Slang">reg_slang2</resource>
	<resource kind="regexp" type="Slang">reg_social_media_en</resource>
	<resource kind="regexp" type="Informal">reg_social_media_en2</resource>
    </module>

    <module path="break-detector">
        <parameters>
            <param name="source-axis">MAIN</param>
            <param name="source-level">HEAD</param>
            <param name="target-axis">TextSegmentAxis</param>
            <param name="target-level">0</param>
            <param name="target-type">Sentence</param>
            <param name="mode">merge</param>
            <param name="block-range">SentenceStart;ParagraphStart;BlockStart</param>
            <param name="chained-rules">true</param>
            <param name="max-words">200</param>
            <param name="max-chars">1000</param>
            <param name="skip-on-sentence-detection">[ Lexeme: EntityType=="Emoticon" ]</param>
            <param name="term-set">[[:SentenceBreak=ATerm:][:SentenceBreak=STerm:]]</param>
            <param name="lower-case-threshold">0.05</param>
            <param name="upper-case-threshold">0.05</param>
        </parameters>
        <resource kind="break-rule">shortcuts</resource>
        <resource kind="break-rule">sentence-common</resource>
        <resource kind="break-rule">sentence-mixed</resource>
        <resource kind="break-rule" status="off">sentence-strict</resource>
        <resource kind="break-rule" group="lower">shortcuts</resource>
        <resource kind="break-rule" group="lower">sentence-common</resource>
        <resource kind="break-rule" group="lower">sentence-lower</resource>
    </module>

