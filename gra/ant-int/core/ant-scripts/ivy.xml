<?xml version="1.0" encoding="ISO-8859-1"?>
<?xml-stylesheet type="text/xsl" href="http://www.jayasoft.fr/org/ivyrep/ivy-doc.xsl"?>
<ivy-module version="2.0" xmlns:e="http://ant.apache.org/ivy/extra">
    <info
        organisation="myorg" 
        module="ant-scripts"
     >
        <ivyauthor name="myorg" url="www.myorg.com"/>
        <description homepage="http://www.myorg.com" />
    </info>
    <configurations>
        <conf name="ant-contrib"/>
        <conf name="junit"/>
    </configurations>
    <publications>
    </publications>
    <dependencies>
        <dependency org="ant-contrib" name="ant-contrib" rev="1.0" conf="ant-contrib->default" transitive="true" />
        <dependency org="junit" name="junit" rev="4.11" conf="junit->*" transitive="true" />
        <dependency org="org.hamcrest" name="hamcrest" rev="1.3" conf="junit->*"/>
    </dependencies>
</ivy-module>
