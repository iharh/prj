        <type name="Person" priority="200" status="on"/>
        <type name="Organization" priority="200" status="on"/>
        <type name="Location" priority="200" status="on"/>
        <type name="NamedObject" priority="200" status="on"/>
        <type name="NamedEntity" priority="300" status="on"/>
        <type name="Company" priority="300" status="on"/>
        <type name="Brand" priority="300" status="on"/>
        <type name="Product" priority="300" status="on"/>
        <type name="DrugsEntity" priority="8" status="off"/>
        <type name="Financial" priority="400" status="on" depends-on="Numeric"/>
        <type name="Phone" priority="70" status="on"/>
        <type name="Address" priority="100" status="on" depends-on="Numeric"/>
        <type name="Email" priority="100" status="on" depends-on="Numeric"/>
        <type name="URL" priority="100" status="on" depends-on="Numeric"/>
        <type name="Date" priority="100" status="on" depends-on="Numeric"/>
        <type name="Age" priority="100" status="on" depends-on="Date"/>
        <type name="Measure" priority="100" status="on"/>
        <type name="Coordinate" priority="100" status="on"/>
        <type name="OtherSpecialEntity" priority="500" status="on"/>
