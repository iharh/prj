    <module path="text-parser.jar">
        <parameters>
            <!-- Text segment break match pattern:
                new line, then one more new line or
                zero-width lookahed for more then two spaces or spaces and dash
              -->
            <param name="plain-text.block">\r?\n(?:\r?\n|(?=(?:[ \t]+(?:\-|[ ]{2,}))))</param>
        </parameters>
    </module>

    <module path="break-detector">
        <parameters>
            <param name="source-axis">MAIN</param>
            <param name="source-level">HEAD</param>
            <param name="target-axis">MAIN</param>
            <param name="target-level">0</param>
            <param name="target-type">Annotation</param>
            <param name="mode">split</param>
            <param name="block-range">SentenceStart;ParagraphStart;BlockStart</param>
            <param name="chained-rules">false</param>
            <param name="max-chars">255</param>
        </parameters>
        <resource kind="break-rule">shortcuts</resource>
        <resource kind="break-rule">token-common</resource>
        <resource kind="break-rule">token-mixed</resource>
        <resource kind="break-rule" status="off">token-fix-quoted</resource>
        <resource kind="break-rule" group="lower">shortcuts</resource>
        <resource kind="break-rule" group="lower">token-common</resource>
        <resource kind="break-rule" group="lower">token-lower</resource>
        <resource kind="break-rule" group="lower" status = "off">token-fix-quoted</resource>
    </module>

    <module path="regexp">
