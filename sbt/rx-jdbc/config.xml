        <resource kind="regexp" type="Phone">reg_phone</resource>
        <resource kind="regexp" type="Coordinate" priority="500">reg_latitude</resource>
        <resource kind="regexp" type="Coordinate" priority="500">reg_longitude</resource>
        <resource kind="regexp" type="OtherSpecialEntity">reg_iban</resource>
        <resource kind="regexp" type="OtherSpecialEntity">reg_passport</resource>
        <resource kind="regexp" type="OtherSpecialEntity">reg_vrn</resource>
