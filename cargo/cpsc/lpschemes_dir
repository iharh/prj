<?xml version="1.0" encoding="utf-8"?>
<scheme name="unknown">

    <import scheme="neutral.xml"/>

    <enum name="SpeechPartEnum">
        <enum-value code="0">Unknown</enum-value>
        <enum-value code="1">Punctuation</enum-value>
        <enum-value code="2">Adverb</enum-value>
        <enum-value code="3">Adjective</enum-value>
        <enum-value code="4">Noun</enum-value>
        <enum-value code="5">Verb</enum-value>
        <enum-value code="6">Introductory</enum-value>
    </enum>

    <enum name="ProperEnum">
        <enum-value code="10">Name</enum-value>
        <enum-value code="11">Surname</enum-value>
        
    </enum>

    <type name="Annotation" inherits="Lexeme">
        <multivalued-group name="GrammarForm">
            <attribute name="SpeechPart" type="SpeechPartEnum"/>
            <attribute name="NormalForm" type="string"/>
            <attribute name="Proper" type="ProperEnum"/>
        </multivalued-group>
    </type>

</scheme>